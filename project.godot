[application]

config/name=".WERF"
run/main_scene="res://scenes/WorldAndRender.tscn"
config/icon="res://project/icon.png"

[audio]

default_bus_layout="res://sound/default_bus_layout.tres"

[autoload]

GameWorld="*res://singletons/GameWorld.tscn"
GlobalVariables="*res://singletons/GlobalVariables.gd"
Settings="*res://singletons/Settings.gd"
ConsoleCommands="*res://singletons/GameSetup.gd"
